{
  "manifest_version": 2,
  "name": "Video Speeder",
  "description": "Scale play speed in video tags.",
  "version": "1.0.0",

  "options_page":"options/options.html",

  "content_scripts": [
    {
      "js":         [ "content.js" ],
      "matches":    [ "<all_urls>" ],
  	  "all_frames": true,
  	  "run_at":     "document_end"
    }
  ],

  "permissions": [
    "storage",
    "contextMenus"
  ],

  "web_accessible_resources": [
    "js/common.js",
    "js/case_vk.js",
    "js/case_youtube.js",
    "js/case_others.js",
    "js/iframe_content.js",
    "css/styles_YT.css",
    "css/styles_VK.css",
    "css/styles_FB.css",
    "css/styles_others.css",
    "options/options.css",
    "options/options.js"
  ]
}
